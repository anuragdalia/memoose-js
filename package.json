{
  "name": "memoose-js",
  "version": "2.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "url": "https://github.com/anuragdalia/memoose-js"
  },
  "author": {
    "name": "Anurag Dalia",
    "url": "https://twitter.com/anuragdalia"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "bun run clean && vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0"
  },
  "optionalDependencies": {
    "ioredis": "^5.4.1",
    "mongoose": "^8.4.4"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.14.9",
    "@types/object-hash": "^3.0.6",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "memoization",
    "caching",
    "performance-enhancement",
    "data-caching",
    "function-caching",
    "cache-management",
    "ttl-cache",
    "nodejs-caching",
    "async-cache",
    "redis-cache",
    "memory-cache",
    "cache-keys",
    "cache-provider",
    "efficient-computing",
    "api-caching",
    "decorator-cache",
    "server-side-cache"
  ]
}
